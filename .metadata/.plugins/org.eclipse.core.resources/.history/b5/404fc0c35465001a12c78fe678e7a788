package entities;

public class Triangle{
	
	public double width;
	public double heigth;
	
	public double area() {
		return (width * heigth)/2;	
	}
    
	public double diagonal() {
        return Math.sqrt((width*width + heigth*heigth));
	}    
    public double perimetro() {
    	return width + heigth + this.diagonal();
		
	}
	
}
