package entities;

public class Persona {
	String name;
	double salary;
	double tax;

	public double salaryBrute() {
        return salary - tax;
	}
	
    public void increaseSalary(int porcentagem){
    	salary += (salary*(porcentagem/100));
    	
    }
	
	
	public String toString() {
		return "Employee: " + this.name + ", $ "+String.format("%.2f",salaryBrute());
	}
	
}
